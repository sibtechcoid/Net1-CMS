{
    "name": "laravel/laravel",
    "type": "project",
    "description": "Josh - Laravel Admin Template with GUI crud builder",
    "keywords": [
        "GUI crud",
        "laravel",
        "dashboard",
        "admin template"
    ],
    "license": "Envato License",
    "repositories": [
        {
            "type": "path",
            "url": "./infyomlabs/laravel-generator"
        },
        {
            "type": "path",
            "url": "./infyomlabs/generator-builder"
        },
        {
            "type": "path",
            "url": "./infyomlabs/core-templates"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-gd": "*",
        "arcanedev/log-viewer": "^5.0",
        "cartalyst/sentinel": "^3.0.0",
        "consoletvs/charts": "^6.5",
        "cviebrock/eloquent-sluggable": "^6.0.0",
        "cviebrock/eloquent-taggable": "^6.0.0",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "infyomlabs/core-templates": "*",
        "infyomlabs/generator-builder": "*",
        "infyomlabs/laravel-generator": "^1.0",
        "infyomlabs/swagger-generator": "dev-master",
        "intervention/image": "^2.4 ",
        "laravel/framework": "^6.0",
        "laravel/helpers": "^1.0",
        "laravel/socialite": "^4.2",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^6.0",
        "maatwebsite/excel": "^3.1",
        "spatie/laravel-activitylog": "^3.8",
        "spatie/laravel-analytics": "^3.8.0",
        "yajra/laravel-datatables-oracle": "^9.6",
      "ext-curl": "*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "facade/ignition": "^1.7",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
